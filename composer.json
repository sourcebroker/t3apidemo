{
  "name": "sourcebroker/t3apidemo",
  "description": "T3api demo",
  "license": "GPL-2.0-or-later",
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "process-timeout": 99999,
    "platform": {
      "php": "7.4.1"
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "./packages/*"
    }
  ],
  "autoload": {
    "psr-4": {
      "V\\Local\\": "public/typo3conf/ext/local/Classes"
    }
  },
  "require": {
    "php": "^7.4.1",
    "ext-json": "*",
    "helhum/typo3-console": "^7.0.0",
    "typo3/minimal": "^11.5",
    "typo3/cms-about": "^11.5",
    "typo3/cms-adminpanel": "^11.5",
    "typo3/cms-belog": "^11.5",
    "typo3/cms-info": "^11.5",
    "typo3/cms-reports": "^11.5",
    "typo3/cms-rte-ckeditor": "^11.5",
    "typo3/cms-setup": "^11.5",
    "typo3/cms-t3editor": "^11.5",
    "typo3/cms-recycler": "^11.5",
    "typo3/cms-tstemplate": "^11.5",
    "typo3/cms-fluid-styled-content": "^11.5",
    "ssch/typo3-encore": "^3.0",
    "fluidtypo3/vhs": "@dev",
    "sourcebroker/local": "@dev",
    "sourcebroker/t3api": "dev-typo3-115-support",
    "sourcebroker/t3apinews": "dev-typo3-115-support",
    "georgringer/news": "dev-master as 8.6.0"

  },
  "scripts": {
    "typo3-cms-scripts": [
      "typo3cms install:fixfolderstructure"
    ],
    "post-autoload-dump": [
      "@typo3-cms-scripts"
    ]
  },
  "require-dev": {
    "rector/rector": "^0.11.57",
    "nimut/testing-framework": "^5.2"
  }
}
